# Orionoid API Credentials (REQUIRED)
# Get your user API key from your Orionoid account settings
ORIONOID_USER_API_KEY=your_orionoid_user_api_key_here

# Service Configuration (OPTIONAL)
SERVICE_PORT=8080
SERVICE_HOST=0.0.0.0

# Prowlarr Authentication (OPTIONAL)
# If set, Prowlarr must provide this API key when making requests
# PROWLARR_API_KEY=your_prowlarr_api_key_here

# Search Configuration (OPTIONAL)
DEFAULT_SEARCH_LIMIT=100
MAX_SEARCH_LIMIT=1000

# Logging Configuration (OPTIONAL)
LOG_LEVEL=INFO

# Cache Configuration (OPTIONAL)
CACHE_TTL=300